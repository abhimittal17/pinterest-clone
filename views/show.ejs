<% include ./partials/header.ejs %>
<div class="w-full min-h-full bg-zinc-800 px-10 py-8 text-white">
  <div class="cards flex flex-wrap gap-10">
    <div class="card w-32">
      <% user.posts.forEach(function(elem){%> 
        <div class="w-32 h-40 bg-sky-400 rounded-lg overflow-hidden">
            <img src="/images/uploads/<%= elem.image %>" class="w-full h-full object-cover" alt=""/>
          </div>
          <h5 class="mt-2 text-sm"><%=elem.title%></h5>
        </div>
        <% })%>
  </div>
</div>
<% include ./partials/footer.ejs %>



